{
  "name": "UnitHegemony",
  "version": "1.0",
  "manifest_version": 1,
  "description": "unit system translation",
  "background": { "scripts": ["main.js"] },
  "browser_action": {
    "default_icon": {
      "19": "images/package.png"
    },
    "default_title": "unit hegemony",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "storage"
  ]
}
